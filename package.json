{
  "name": "@makeflow/sdk",
  "version": "0.1.0-beta.6",
  "description": "Makeflow SDK",
  "repository": "https://github.com/makeflow/makeflow-sdk",
  "author": "Chengdu Mufan Technology Co., Ltd.",
  "main": "bld/power-app/index.js",
  "bin": {
    "mf": "bld/program/cli.js"
  },
  "files": [
    "src/program/**/*.ts",
    "bld/**"
  ],
  "scripts": {
    "prepublishOnly": "yarn build-ts",
    "prerelease": "npm version prerelease --preid=beta",
    "run:app-builder": "cd src/app-builder && yarn start",
    "build:program": "yarn tsc --project src/program",
    "build:power-app": "yarn tsc --project src/power-app",
    "build:power-app-schema": "typescript-json-schema --required --out bld/power-app-schema/schema.json src/power-app-schema/tsconfig.json PowerAppSchema",
    "build-ts": "rimraf bld &&  yarn build:program && yarn build:power-app && mkdir bld/power-app-schema && yarn build:power-app-schema",
    "lint-ts": "ts-node --transpile-only --project scripts/tsconfig.json scripts/lint-ts-projects.ts",
    "lint-prettier": "prettier --check \"**/*.{ts,tsx,js,jsx,json,yml,md}\"",
    "lint": "yarn lint-prettier && yarn lint-ts"
  },
  "devDependencies": {
    "@magicspace/configs": "^0.2.3",
    "@magicspace/eslint-plugin": "^0.1.23",
    "@types/jsonschema": "^1.1.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.4.0",
    "@types/lodash": "^4.14.149",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^11.11.8",
    "@types/prettier": "^2.0.0",
    "@types/prompts": "^1.2.0",
    "@types/rc": "^1.1.0",
    "@types/semver": "^6.2.0",
    "@types/strip-json-comments": "^3.0.0",
    "biu": "^0.2.20",
    "eslint": "^7.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^2.6.3",
    "strip-json-comments": "^3.1.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.7.4",
    "typescript-json-schema": "^0.36.0"
  },
  "dependencies": {
    "@makeflow/types": "^0.1.6",
    "@makeflow/types-nominal": "^0.1.0",
    "@types/koa": "^2.11.0",
    "@types/mongodb": "^3.5.18",
    "@types/node-fetch": "^2.5.7",
    "clime": "^0.5.10",
    "fp-ts": "^2.4.0",
    "io-ts": "^2.0.4",
    "io-ts-types": "^0.5.5",
    "jsonschema": "^1.2.4",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.15",
    "lowdb": "^1.0.0",
    "mongodb": "^3.4.1",
    "node-fetch": "^2.6.0",
    "prompts": "^2.0.4",
    "semver": "^7.1.1",
    "tslang": "^0.1.13",
    "villa": "^0.3.1"
  }
}
